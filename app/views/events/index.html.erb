<%= link_to "診察予約日を登録する", new_member_event_path %>

<% @events.each do |event| %>
  <%= event.start_time %>
  <%= event.title %>

<% end %>

<%= month_calendar events: @events do |date, events| %>
  <%= date.day %>

  <% events.each do |event| %>
    <div>
      <%= link_to event.title %>
    </div>
  <% end %>
<% end %>