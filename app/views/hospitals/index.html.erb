<h1>病院一覧</h1>

<%= form_with(url: search_path, method: :get, local: true) do |f| %>
  <%= f.check_box :search, {multiple: true, include_hidden: false}, 'a',nil %>
  <%= f.submit 'Search' %>
<% end %>

<% @hospitals.each do |hospital| %>
  <%= link_to hospital.name, hospital_path(hospital) %>
<% end %>